[general]
status_path = "~/.vdirsyncer/status/"

[pair calendars]
conflict_resolution = "b wins"
a = "calendars_local"
b = "calendars_dav"
collections = ["from b"]
metadata = ["color", "displayname"]

[storage calendars_local]
type = "filesystem"
path = "~/.calendars/"
fileext = ".ics"

[storage calendars_dav]
type = "caldav"
url = "https://cloud.disroot.org/"
username.fetch = ["shell", "rbw list --fields name,user | grep Disroot | cut -f 2"]
password.fetch = ["shell", "rbw get Disroot"] 
